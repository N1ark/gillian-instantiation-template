spec test_store(x)
  [[ (x == #x) * <points_to>(;#any) ]]
  [[ (ret == null) * <points_to>(;#x) ]]
  normal
proc test_store(x) {
  e := [store](x);
  ret := null;
  return
};

spec test_invalid_store(x)
  [[ (x == #x) ]]
  [[ (ret == null) ]]
  normal
proc test_invalid_store(x) {
  e := [store](x);
  ret := null;
  return
};

